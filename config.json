{
   "fetcher": {
    "instances": 5,
    "request": {
        "jar": true,
        "timeout": 30000,
        "followRedirect": true,
        "maxRedirects": 3,
        "headers": {
          "User-Agent": "crawl.js v0.0.1"
        }
    }
  },
  "extractor": "parser",
  "queues": {
    "local": {
      "type": "memory",
      "options": {
        "limit": 1000
      }
    },
    "remote": {
      "type": "redis",
      "options": {
        "flushInterval": 30000,
        "host": "127.0.0.1",
        "port": "6379"
      }
    }
  },
  "seen": {
    "host": "127.0.0.1",
    "port": "6379"
  },
  "dispatcher": {
    "acceptPattern": ""
  },
  "store": {
    "type": "dummy",
    "options": {}
  },
  "url": {
    "blocks": 1 
  },
  "robo": {
    "limit": 100
  }
}
