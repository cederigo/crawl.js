{
    "interval": 1000
  , "block": 0
  , "fetchers": 1
  , "fetcher": {
        "timeout": 30000
      , "followRedirect": true
      , "maxRedirects": 20
      , "headers":{
          "User-Agent": "crawl.js v0.1"
        }
    }
  , "extractor": "Parser"
  , "storage": {
        "type": "riak"
      , "options": {
            "host": "127.0.0.1"
          , "port": "8098"
      }
    }
  , "queue": {
    "filter": { "type": "mine" }
  }
}
